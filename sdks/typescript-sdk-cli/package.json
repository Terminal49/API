{
  "name": "@terminal49/cli",
  "version": "0.1.0",
  "description": "Terminal49 container tracking CLI â€” for LLM agents, chat interfaces, and humans",
  "type": "module",
  "bin": {
    "t49": "./dist/bin/t49.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
