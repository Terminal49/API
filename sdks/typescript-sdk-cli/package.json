  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@terminal49/sdk": "^0.1.0",
    "commander": "^13.0.0",
    "chalk": "^5.4.0",
    "cli-table3": "^0.6.5"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "vitest": "^4.0.13",
    "@vitest/coverage-v8": "^4.0.18",
